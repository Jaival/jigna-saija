{
  "env": {
    "browser": true,
    "node": true,
    "es2022": true
  },
  "extends": [
    "next/core-web-vitals",
    "plugin:@next/next/recommended",
    "eslint:recommended",
    "next"
  ],
  "rules": {
    "no-unused-vars": "off",
    "indent": [
      "warn",
      2
    ],
    "quotes": [
      "error",
      "single"
    ]
  }
}